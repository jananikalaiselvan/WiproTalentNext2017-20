package project;
public class Main
{
    public static void main(String[] args)
    {
		  int n=args.length;
		  if(args.length==0)
	  	{
		  	System.out.println("No Values");
	   	}
	  	else
		  {
			  for(int i=0;i<n-1;i++)
			   {
				  System.out.print(args[i]+",");
			   }
		   	System.out.print(args[n-1]);
	  	}

  	}
}
