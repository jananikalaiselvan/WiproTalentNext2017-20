import java.util.Scanner;
 
class Main
{
   public static void main(String args[])
   {
      int n,num,d, r=0;
     
      Scanner in = new Scanner(System.in);
      n = in.nextInt();
      num=n;
      
      while(n!=0)
      {
          d=n%10;
          r=r*10+d;
          n=n/10;
         
      }
       if(num==r)
       {
           System.out.println(num+" is a palindrome");
       }
       else
       {
           System.out.println(num+" is not a palindrome");
       }
   }
}
