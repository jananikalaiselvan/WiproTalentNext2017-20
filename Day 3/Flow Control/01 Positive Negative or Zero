import java.util.Scanner;
public class Main
{
	public static void main(String[] args)
  {
		     Scanner d=new Scanner(System.in);
         int n1 =d.nextInt();
         if(n1<0)
         {
            System.out.println("Negative"); 
         }
         else if(n1>0)
         {
             System.out.println("Positive");
         }
         else
         {
             System.out.println("Zero");
         }
	}
}
