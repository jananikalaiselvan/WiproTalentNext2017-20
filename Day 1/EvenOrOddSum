import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner s=new Scanner(System.in);
         int n1=s.nextInt();
         int n2=s.nextInt();
         int n=n1+n2;
         if(n%2==0)
         {
            System.out.println("Sum is Even number"); 
         }
         else
         {
             System.out.println("Sum is Odd number");
         }
	}
}
