import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in); 
	    int[] a=new int[25];
	    int i=0;
	    int max=0;
	    while(i<25)
	    {
	        a[i]=sc.nextInt();
	        if(max<a[i])
	        {
	            max=a[i];
	        }
	        i++;
	    }
	    System.out.println(max);
	 }
}
