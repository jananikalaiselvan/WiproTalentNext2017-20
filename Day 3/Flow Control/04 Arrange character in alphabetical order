import java.util.*;
class Main
{
    public static void main (String[] args)
    {
        Scanner d= new Scanner(System.in);
        char input1=d.next().charAt(0);
        char input2=d.next().charAt(0);
        if (input1>input2)
            System.out.println(input2+" , "+input1);
        else
            System.out.println(input1+" , "+input2);
    }
}
