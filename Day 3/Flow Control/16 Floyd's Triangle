import java.util.Scanner;
 
class Main
{
   public static void main(String args[])
   {
      int num, n = 1, a, b;
     
      Scanner in = new Scanner(System.in);
      num = in.nextInt();
      
      for ( a = 1 ; a <= num; a++ )
      {
         for ( b = 1 ; b <= a ; b++ )
           {
              System.out.print("*"+" ");
              n++;
           }
          System.out.println();
      }
      
      }
}
